  Person: 0
  Photo: {fileID: 21300000, guid: cc417bff073d11246a8a207177244c41, type: 3}
  Info: "\u0412\u0430\u0441\u0438\u043B\u0438\u0439 \u0424\u0451\u0434\u043E\u0440\u043E\u0432\u0438\u0447
    \u041B\u0430\u043F\u0448\u0438\u043D (1844 \u2014 1919) \u2014 \u043F\u0440\u0435\u0434\u043F\u0440\u0438\u043D\u0438\u043C\u0430\u0442\u0435\u043B\u044C,
    \u043A\u0443\u043F\u0435\u0446 \u043F\u0435\u0440\u0432\u043E\u0439 \u0433\u0438\u043B\u044C\u0434\u0438\u0438,
    \u043C\u0435\u0446\u0435\u043D\u0430\u0442, \u043F\u043E\u0442\u043E\u043C\u0441\u0442\u0432\u0435\u043D\u043D\u044B\u0439
    \u043F\u043E\u0447\u0451\u0442\u043D\u044B\u0439 \u0433\u0440\u0430\u0436\u0434\u0430\u043D\u0438\u043D
    \u0426\u0430\u0440\u0438\u0446\u044B\u043D\u0430, \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0442\u0435\u043B\u044C\u043D\u044B\u0439
    \u0441\u0442\u0430\u0442\u0441\u043A\u0438\u0439 \u0441\u043E\u0432\u0435\u0442\u043D\u0438\u043A,
    \u0433\u043B\u0430\u0432\u0430 \u0426\u0430\u0440\u0438\u0446\u044B\u043D\u0430,
    \u0433\u043B\u0430\u0441\u043D\u044B\u0439 \u0446\u0430\u0440\u0438\u0446\u044B\u043D\u0441\u043A\u043E\u0439
    \u0433\u043E\u0440\u043E\u0434\u0441\u043A\u043E\u0439 \u0434\u0443\u043C\u044B.
    \u041E\u0441\u043D\u043E\u0432\u0430\u0442\u0435\u043B\u044C \u043A\u043E\u043D\u0434\u0438\u0442\u0435\u0440\u0441\u043A\u043E\u0433\u043E
    \u043F\u0440\u0435\u0434\u043F\u0440\u0438\u044F\u0442\u0438\u044F, \u043D\u044B\u043D\u0435
    \u0438\u043C\u0435\u043D\u0443\u0435\u043C\u043E\u0433\u043E \xAB\u041A\u043E\u043D\u0444\u0438\u043B\xBB.
    \u0420\u043E\u0434\u0438\u043B\u0441\u044F \u0432 1844 \u0433\u043E\u0434\u0443
    \u0432 \u043A\u0443\u043F\u0435\u0447\u0435\u0441\u043A\u043E\u0439 \u0441\u0435\u043C\u044C\u0435.
    \u0412 \u0441\u0435\u0440\u0435\u0434\u0438\u043D\u0435 XIX \u0432\u0435\u043A\u0430
    \u0435\u0433\u043E \u0441\u0442\u0430\u0440\u0448\u0438\u0435 \u0431\u0440\u0430\u0442\u044C\u044F
    \u043F\u0435\u0440\u0435\u0431\u0440\u0430\u043B\u0438\u0441\u044C \u0432 \u0433\u043E\u0440\u043E\u0434
    \u041C\u0430\u043A\u0430\u0440\u044C\u0435\u0432 \u041A\u043E\u0441\u0442\u0440\u043E\u043C\u0441\u043A\u043E\u0439
    \u0433\u0443\u0431\u0435\u0440\u043D\u0438\u0438, \u0433\u0434\u0435 \u043E\u0441\u043D\u043E\u0432\u0430\u043B\u0438
    \u043F\u0440\u043E\u0438\u0437\u0432\u043E\u0434\u0441\u0442\u0432\u043E \u043A\u043E\u043D\u0434\u0438\u0442\u0435\u0440\u0441\u043A\u0438\u0445
    \u0438\u0437\u0434\u0435\u043B\u0438\u0439 \u0438 \u0442\u043E\u0440\u0433\u043E\u0432\u043B\u044E
    \u043A\u043E\u043B\u043E\u043D\u0438\u0430\u043B\u044C\u043D\u044B\u043C\u0438
    \u0442\u043E\u0432\u0430\u0440\u0430\u043C\u0438. \u0412\u0430\u0441\u0438\u043B\u0438\u0439
    \u0436\u0435 \u0437\u0430 \u0431\u0440\u0430\u0442\u044C\u044F\u043C\u0438 \u043D\u0435
    \u043F\u043E\u0441\u043B\u0435\u0434\u043E\u0432\u0430\u043B \u0438 \u043E\u0441\u0442\u0430\u043B\u0441\u044F
    \u0432 \u0426\u0430\u0440\u0438\u0446\u044B\u043D\u0435. \u0415\u0433\u043E \u043E\u0442\u0435\u0446
    \u0431\u044B\u043B \u0431\u043E\u0433\u0430\u0442\u044B\u043C \u0447\u0435\u043B\u043E\u0432\u0435\u043A\u043E\u043C,
    \u043D\u043E \u043C\u043B\u0430\u0434\u0448\u0435\u043C\u0443 \u0441\u044B\u043D\u0443
    \u043E\u0441\u0442\u0430\u0432\u0438\u043B \u043C\u0430\u043B\u0435\u043D\u044C\u043A\u043E\u0435
    \u043D\u0430\u0441\u043B\u0435\u0434\u0441\u0442\u0432\u043E, \u043D\u0430\u043F\u043E\u0441\u043B\u0435\u0434\u043E\u043A
    \u0441\u043A\u0430\u0437\u0430\u0432: \xAB\u0421\u043C\u043E\u0436\u0435\u0448\u044C
    \u0441 \u044D\u0442\u043E\u0433\u043E \u0440\u0430\u0437\u0431\u043E\u0433\u0430\u0442\u0435\u0442\u044C,
    \u0431\u0443\u0434\u0435\u0442 \u0438\u0437 \u0442\u0435\u0431\u044F \u0442\u043E\u043B\u043A,
    \u0430 \u0435\u0441\u043B\u0438 \u043D\u0435\u0442, \u0442\u043E \u043D\u0438\u043A\u0430\u043A\u0438\u0435
    \u0434\u0435\u043D\u044C\u0433\u0438 \u043D\u0435 \u043F\u043E\u043C\u043E\u0433\u0443\u0442!"
  Tasks: 00000000010000000200000003000000
  TasksText: "\u0418\u0442\u0430\u043A, \u0431\u043B\u0438\u0436\u0430\u0439\u0448\u0438\u0439
    \u043F\u043B\u0430\u043D \u043D\u0430\u043C\u0435\u0447\u0435\u043D. \u0423 \u0443\u0432\u0430\u0436\u0430\u0435\u043C\u043E\u0433\u043E
    \u043A\u0443\u043F\u0446\u0430 \u041B\u0430\u043F\u0448\u0438\u043D\u0430 \u0441\u0432\u043E\u0431\u043E\u0434\u043D\u043E\u0433\u043E
    \u0432\u0440\u0435\u043C\u0435\u043D\u0438 \u043D\u0435 \u0442\u0430\u043A \u0443\u0436
    \u0438 \u043C\u043D\u043E\u0433\u043E. \u041D\u0430 \u0441\u0435\u0433\u043E\u0434\u043D\u044F
    \u0443 \u043D\u0435\u0433\u043E \u0437\u0430\u043F\u043B\u0430\u043D\u0438\u0440\u043E\u0432\u0430\u043D\u043E
    \u0441\u043B\u0435\u0434\u0443\u044E\u0449\u0435\u0435: \r\n - \u0421\u043D\u044F\u0442\u044C
    \u0434\u0435\u043D\u044C\u0433\u0438 \u0432 \u0431\u0430\u043D\u043A\u0435;\n-
    \u041F\u043E\u0441\u0435\u0442\u0438\u0442\u044C \u043D\u0430\u0440\u043E\u0434\u043D\u043E\u0435
    \u0441\u043E\u0431\u0440\u0430\u043D\u0438\u0435;\n - \u0417\u0430\u0431\u0440\u0430\u0442\u044C
    \u0434\u043E\u0447\u044C \u0438\u0437 \u0433\u0438\u043C\u043D\u0430\u0437\u0438\u0438;\n-
    \u0417\u0430\u0439\u0442\u0438 \u043A \u043A\u0443\u043F\u0446\u0443 \u041C\u0438\u0448\u0438\u043D\u0443.\n\u041D\u0430\u0436\u043C\u0438
    \u043D\u0430 \u043A\u043D\u043E\u043F\u043A\u0443 \"\u0412 \u043F\u0443\u0442\u044C\",
    \u043D\u0430 \u0442\u0432\u043E\u0451\u043C \u0443\u0441\u0442\u0440\u043E\u0439\u0441\u0442\u0432\u0435
    \u043E\u0442\u043A\u0440\u043E\u0435\u0442\u0441\u044F \u0431\u0440\u0430\u0443\u0437\u0435\u0440
    \u0441 \u043D\u0430\u0432\u0438\u0433\u0430\u0442\u043E\u0440\u043E\u043C \u0434\u043E
    \u043F\u0435\u0440\u0432\u043E\u0439 \u0442\u043E\u0447\u043A\u0438 \u043C\u0430\u0440\u0448\u0440\u0443\u0442\u0430.
    \u041A\u0430\u043A \u0434\u043E\u0431\u0435\u0440\u0451\u0448\u044C\u0441\u044F,
    \u0432\u043E\u0437\u0432\u0440\u0430\u0449\u0438\u0439\u0441\u044F \u0432 \u043F\u0440\u0438\u043B\u043E\u0436\u0435\u043D\u0438\u0435
    \u0438 \u043D\u0430\u0436\u0438\u043C\u0430\u0439 \u043A\u043D\u043E\u043F\u043A\u0443
    \"\u0420\u0435\u0436\u0438\u043C AR\". \u041D\u0430\u0432\u0435\u0434\u0438 \u043A\u0430\u043C\u0435\u0440\u0443
    \u043D\u0430 \u0437\u0434\u0430\u043D\u0438\u0435 \u0438 \u043D\u0430\u0436\u043C\u0438
    \u043D\u0430 \u0432\u0438\u0440\u0442\u0443\u0430\u043B\u044C\u043D\u0443\u044E
    \u043A\u043D\u043E\u043F\u043A\u0443, \u0447\u0442\u043E\u0431\u044B \u043F\u043E\u0434\u0442\u0432\u0435\u0440\u0434\u0438\u0442\u044C,
    \u0447\u0442\u043E \u0442\u044B \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0442\u0435\u043B\u044C\u043D\u043E
    \u0440\u044F\u0434\u043E\u043C \u0441 \u043F\u0443\u043D\u043A\u0442\u043E\u043C
    \u043D\u0430\u0437\u043D\u0430\u0447\u0435\u043D\u0438\u044F."
  EndMessage: "\u041D\u0443 \u0432\u043E\u0442 \u0438 \u0432\u0441\u0435, \u043D\u0430\u0448\u0435
    \u043D\u0435\u0431\u043E\u043B\u044C\u0448\u043E\u0435 \u043F\u0443\u0442\u0435\u0448\u0435\u0441\u0442\u0432\u0438\u0435
    \u043F\u043E\u0434\u0445\u043E\u0434\u0438\u0442 \u043A \u043A\u043E\u043D\u0446\u0443.
    \u041D\u0430\u0434\u0435\u044E\u0441\u044C, \u0447\u0442\u043E \u0442\u0435\u0431\u0435
    \u0431\u044B\u043B\u043E \u0438\u043D\u0442\u0435\u0440\u0435\u0441\u043D\u043E
    \u0438 \u0442\u044B \u043D\u0435 \u0441\u0438\u043B\u044C\u043D\u043E \u0443\u0441\u0442\u0430\u043B!
    \u041A\u0430\u043A \u0442\u044B \u043C\u043E\u0433 \u0437\u0430\u043C\u0435\u0442\u0438\u0442\u044C,
    \u0433\u043E\u0440\u043E\u0434 \u0441\u0438\u043B\u044C\u043D\u043E \u0438\u0437\u043C\u0435\u043D\u0438\u043B\u0441\u044F
    \u0441\u043E \u0432\u0440\u0435\u043C\u0451\u043D \u0446\u0430\u0440\u0435\u0439
    \u0438 \u043A\u0443\u043F\u0446\u043E\u0432. \u0427\u0442\u043E \u0442\u0435\u0431\u0435
    \u0437\u0430\u043F\u043E\u043C\u043D\u0438\u043B\u043E\u0441\u044C \u0431\u043E\u043B\u044C\u0448\u0435
    \u0432\u0441\u0435\u0433\u043E? \u041A\u0430\u043A \u0434\u0443\u043C\u0430\u0435\u0448\u044C,
    \u0447\u0442\u043E \u0431\u044B \u0441\u043A\u0430\u0437\u0430\u043B \u0412\u0438\u043A\u0442\u043E\u0440
    \u0424\u0451\u0434\u043E\u0440\u043E\u0432\u0438\u0447, \u043E\u043A\u0430\u0436\u0438\u0441\u044C
    \u043E\u043D \u0432 \u0441\u043E\u0432\u0440\u0435\u043C\u0435\u043D\u043D\u043E\u0439
    \u0420\u043E\u0441\u0441\u0438\u0438? \u041D\u0430\u043F\u0438\u0448\u0438 \u0441\u0432\u043E\u0438
    \u043C\u044B\u0441\u043B\u0438 \u0432 \u043F\u043E\u043B\u0435 \u043D\u0438\u0436\u0435."
